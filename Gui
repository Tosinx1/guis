loopinvis.Name = "loop invis"
loopinvis.Parent = ScrollingFrame
loopinvis.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
loopinvis.Position = UDim2.new(0.0937122852, 0, 0.827206671, 0)
loopinvis.Size = UDim2.new(0, 540, 0, 53)
loopinvis.Font = Enum.Font.SourceSans
loopinvis.Text = "[Loop invis]                                                                                                            (if you die u gonna have to rejoin lol)"
loopinvis.TextColor3 = Color3.fromRGB(0, 0, 0)
loopinvis.TextScaled = true
loopinvis.TextSize = 14.000
loopinvis.TextWrapped = true
loopinvis.MouseButton1Down:connect(function()
	_G.on = 1
	while _G.on == 1
	do
		local args = {
			[1] = {
				[1] = "Skill_Invisible",
				[2] = "Start"
			}
		}

		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		game:GetService("Players").LocalPlayer.Character.Head.InvisibilityUseSound:Destroy()
	end
end)
	

UICorner_24.Parent = loopinvis

