-- Scripts:
local function SNLWZP_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local plr = game.Players.LocalPlayer
	
	
	plr:GetMouse().KeyDown:Connect(function(K)
		if K == "q" then
			script.Parent.Visible = not script.Parent.Visible
		end
	end)
end
coroutine.wrap(SNLWZP_fake_script)()
local function KWMJ_fake_script() -- togglegui.LocalScript 
	local script = Instance.new('LocalScript', togglegui)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ScrollingFrame.Visible = not script.Parent.Parent.ScrollingFrame.Visible
	end)
	
end
coroutine.wrap(KWMJ_fake_script)()
